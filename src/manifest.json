{

    "manifest_version": 2,
    "name": "Privonyx",
    "version": "1.1",

    "description": "Dynamic dark theme for Firefox with a focus on privacy",

    "permissions": [
        "tabs",
        "theme"
    ],
    "applications": {
        "gecko": {
            "strict_min_version": "60.0a1"
        }
    },
    "background": {
        "scripts": ["privonyx.js"]
    }

}
